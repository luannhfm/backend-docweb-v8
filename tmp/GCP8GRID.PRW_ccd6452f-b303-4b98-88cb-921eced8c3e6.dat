User Function GCP8GRID()        
Local aRet
Local lMark    := PARAMIXB[1], ; 
      cDescProd:= PARAMIXB[2], ;
      cSTR0032 := PARAMIXB[3], ;
      cSTR0033 := PARAMIXB[4]
                                                   
Select ("CO2")

aRet := {Iif(CO2->CO2_FRACAS=="S", lMark := .T.,lMark := .F.),;
	              Iif(CO2->CO2_FRACAS=="S",cSTR0032,cSTR0033),; //"Revogado","Não Revogado"
				  CO2->CO2_ITEM,;
				  CO2->CO2_CODPRO,;            
				  cDescProd ,;
				  CO2->CO2_QUANT,; //				  iif(CO2->CO2_SLDEST > 0, CO2->CO2_SLDEST, CO2->CO2_QUANT),;
				  CO2->CO2_QUANT - CO2->CO2_SLDEST,; //-- Quantidade jah atendida do item
				  CO2->CO2_SLDEST,;					//-- Saldo do item do edital
			      CO2->CO2_LOTE,;
			      CO2->CO2_CODEDT,;
			      CO2->CO2_OBS}


Return (aRet)