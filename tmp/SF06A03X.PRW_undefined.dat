/*/{Protheus.doc} ModelDef
(long_description)
@type function
@author Luiz
@since 20/09/2016
@version 1.0
@return ${return}, ${return_description}
@example
(examples)
@see (links_or_references)
/*/

Static Function ModelDef()
                                                                                               
	Local oStruZF2 := FWFormStruct(1,"ZF2")
	Local oModel   := MPFormModel():New("F06A03X")
	
	oModel:AddFields("ZF2UNICO", Nil  , oStruZF2)	
	oModel:SetDescription("Cadastro de Cartões")	
	oModel:GetModel("ZF2UNICO"):SetDescription("Cadastro de Cartões")	
	oModel:SetPrimaryKey({"ZF2_FILIAL","ZF2_CODFOR","ZF2_LOJA","ZF2_BCO"})

Return(oModel) 

/*/{Protheus.doc} ViewDef
(long_description)
@type function
@author Luiz
@since 20/09/2016
@version 1.0
@return ${return}, ${return_description}
@example
(examples)
@see (links_or_references)
/*/

Static Function ViewDef()

	Local oModel   := FWLoadModel("SF06A03X")
	Local oStruZF2 := FWFormStruct(2,"ZF2") 
	Local oView	   := FWFormView():New()
	
	oView:SetModel(oModel)
	oView:AddField("VIEW_ZF2", oStruZF2, "ZF2UNICO")
	oView:CreateHorizontalBox("UM", 100)
	oView:SetOwnerView("VIEW_ZF2" , "UM")

Return(oView)