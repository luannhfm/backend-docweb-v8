User Function SF01G01J()

	//aCols
	If ExistTrigger('N1_LOCAL') // verifica se existe trigger para este campo
		If FunName() == "ATFA060" //Transferencia
			RunTrigger(1,nLin,nil,,'N1_LOCAL')
		EndIf
	Endif

Return( Nil )