User Function FTVD7031

	Local cEstat := U_SFGN001A(ProcName(0), "FTVD7031")
	Local oWnd  := GetWndDefault()
	Local lTela := ParamIXB[1]
	
	For nI := 1 To Len(oWnd:aControls)
	
		If lTela		
			If ValType(oWnd:aControls[nI]) = 'O' .And. Upper(AllTrim(Substr(oWnd:aControls[nI]:cCaption,2))) == 'CHEQUE'
				oWnd:aControls[nI]:lActive := .F.
			EndIf			
			If ValType(oWnd:aControls[nI]) = 'O' .And. Upper(AllTrim(Substr(oWnd:aControls[nI]:cCaption,2))) == '&OND.NEGOCIADA'
				oWnd:aControls[nI]:lActive := .F.
			EndIf		
		EndIf	
	
	Next nI

Return