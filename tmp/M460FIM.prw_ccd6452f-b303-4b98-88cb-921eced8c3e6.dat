User Function M460FIM

    If !Empty(SC5->C5_MDCONTR) .and. SE1->E1_NUM == SC5->C5_NOTA
        RecLock("SE1",.f.)
            SE1->E1_MDCONTR := SC5->C5_MDCONTR
            SE1->E1_XMDCONT := Posicione("CN9", 1, SC5->C5_FILIAL + SC5->C5_MDCONTR, "CN9_XNROCV")
            SE1->E1_XOBSBOL	:= Alltrim(SC5->C5_MDCONTR) +"|"+ Alltrim(SE1->E1_NOMCLI)
        SE1->(MsUnlock())
    EndIf

Return
