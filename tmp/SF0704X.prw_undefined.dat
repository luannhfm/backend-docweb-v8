User Function SF0704X(_cData)
Local _dRet
Local _cYea
Local _cMon

If Len(_cData) == 8
	_cYea	:= subStr(_cData, 5, 4)
	_cMon	:= subStr(_cData, 3, 2)
	_cData	:= subStr(_cData, 1, 2)+ "/" + _cMon + "/" + _cYea  	
Endif

_dRet := cToD(_cData)

Return _dRet