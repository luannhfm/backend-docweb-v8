Titulo += " ("+StrZero( aReturn[8] , 2 )+")"+If(!Empty(cCabec)," - "+cCabec,"") + IF(cTipoRel="A",STR0041,STR0042) + " / " + Upper(MesExtenso(Month(dDtRef)))+STR0059+STR(YEAR(dDtRef),4)   //Analitica###Sintetica

If LastKey() = 27 .Or. nLastKey = 27
