@since 20/09/2016
@version 1.0
@return ${return}, ${return_description}
@example
(examples)
@see (links_or_references)
/*/

Static Function ModelDef()
                                                                                               
	Local oStruZF3 := FWFormStruct(1,"ZF3")
	Local oModel   := MPFormModel():New("F06A09X")
	
	oModel:AddFields("ZF3UNICO", Nil  , oStruZF3)	
	oModel:SetDescription("Cadastro de Cartões")	
	oModel:GetModel("ZF3UNICO"):SetDescription("Cadastro de Cartões")	
	oModel:SetPrimaryKey({"ZF3_FILCAD","ZF3_ENT","ZF3_ASSOC"})

Return(oModel) 

/*/{Protheus.doc} ViewDef
(long_description)
@type function
@author Luiz
@since 20/09/2016
@version 1.0
@return ${return}, ${return_description}
@example
(examples)
@see (links_or_references)
/*/

Static Function ViewDef()

	Local oModel   := FWLoadModel("SF06A09X")
	Local oStruZF3 := FWFormStruct(2,"ZF3") 
	Local oView	   := FWFormView():New()
	
	oView:SetModel(oModel)
	oView:AddField("VIEW_ZF3", oStruZF3, "ZF3UNICO")
	oView:CreateHorizontalBox("UM", 100)
	oView:SetOwnerView("VIEW_ZF3" , "UM")

Return(oView)