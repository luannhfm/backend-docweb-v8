User Function GCP2VALL()        
Local lRet := .T.                                       
Local cProdut := PARAMIXB[1], nQtde  := PARAMIXB[2]                     

if __lGCPCNI1 
  	If Empty(cProdut) 
		Help("",1,"GCP02NOLIN") //"Preenchimento de todos os campos do produto obrigatório"
	  	lRet := .F.
	Endif
Else
	If Empty(cProdut) .Or. nQtde == 0
		Help("",1,"GCP02NOLIN") //"Preenchimento de todos os campos do produto obrigatório"
		lRet := .F.
	Endif
EndIf

Return (lRet)