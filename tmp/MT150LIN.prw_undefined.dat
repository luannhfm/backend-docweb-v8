#Include "Protheus.ch"

User Function MT150LIN()

   Local _aArea    := GetArea()
   Local _aAreaB1  := SB1->(GetArea())
   Local _nPosProd := aScan(aHeader, {|x| Alltrim(x[2]) == "C8_PRODUTO"})
   Local _nPosDesc := aScan(aHeader, {|x| Alltrim(x[2]) == "C8_XXPRODU"})
   Local _nPosXXDe := aScan(aHeader, {|x| Alltrim(x[2]) == "C8_XXDESCR"})
   Local _nPosXXMa := aScan(aHeader, {|x| Alltrim(x[2]) == "C8_XXMARCA"})

   dbSelectArea("SB1")
   SB1->(dbSetOrder(1))
   If SB1->(dbSeek(xFilial("SB1")+aCols[n][_nPosProd]))
      aCols[n][_nPosDesc] := SB1->B1_DESC
      aCols[n][_nPosXXDe] := SB1->B1_XXDESCR
   Endif
   
   If L150INCLUI
   		aCols[n][_nPosXXMa] := Space(TamSX3("C8_XXMARCA")[1])	
   EndIf
   
   RestArea(_aAreaB1)
   RestArea(_aArea)   
   
Return NiL