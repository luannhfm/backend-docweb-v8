#Include 'Protheus.ch'

User Function SF01A04X()
Local _cSol := ""
Local _cAliasTMP	:= GetNextAlias()

_cQuery := "  Select Max(NM_XSOLIC) As NM_XSOLIC From " + RetSQLName("SNM") + " "

MemoWrite("C:\temp\"+FunName()+"_"+ProcName()+".TXT",_cQuery)

DBUseArea(.T., "TOPCONN", TcGenQry(,,ChangeQuery(_cQuery)),_cAliasTMP, .F.,.F.)

If !(_cAliasTMP)->(Eof())
	_cSol := (_cAliasTMP)->NM_XSOLIC
	_cSol := iIf( !Empty(_cSol), StrZero((Val(_cSol)+1 ), 6),"000001") 
EndIf 

Return _cSol

