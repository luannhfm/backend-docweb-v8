Static Function fCarrega()

Local _nReg   := 0 
Local _cParam := ""
Local _cCont  := ""

DbSelectArea("ZAO")
ZAO->(DbSetOrder(1))
ZAO->(DbEval({||_nReg++}))

If _nReg != 13
    
	//Retorna ao primeiro registro
	ZAO->(DbGoTop())
	
	//Excluindo todos os registros da tabela.
	While ZAO->(!Eof())	
		If RecLock("ZAO",.F.)
			ZAO->(DbDelete())
			ZAO->(MsUnLock())
		EndIF
		ZAO->(DbSkip())	
	EndDo
	
	//Criando a ZAO padrao
	For _nX := 1 To 13
		If RecLock("ZAO",.T.)
			_cParam := ""
			_cCont  := ""
			Do Case
				Case _nX = 1
					_cParam := "PARAM01"
					_cCont  := "1"	
				Case _nX = 2
					_cParam := "PARAM02"
					_cCont  := "1"
				Case _nX = 3
					_cParam := "PARAM03"
					_cCont  := "2"
				Case _nX = 4
					_cParam := "PARAM04"
					_cCont  := "REN"
				Case _nX = 5
					_cParam := "PARAM05"
					_cCont  := ""
				Case _nX = 6
					_cParam := "PARAM06"
					_cCont  := "FI"
				Case _nX = 7
					_cParam := "PARAM07"
					_cCont  := "10"
				Case _nX = 8
					_cParam := "PARAM08"
					_cCont  := "FI"
				Case _nX = 9
					_cParam := "PARAM09"
					_cCont  := "45"
				Case _nX = 10 // vencimento maximo para titulos muito atrasados 
					_cParam := "PARAM10"
					_cCont  := "20071231"
				Case _nx = 11 // percentual maximo de desconto para titulos muito atrasados 
					_cParam := "PARAM11"
					_cCont  := "40"
				Case _nx = 12 // percentual minimo para a entrada em titulos muito vencidos
					_cParam := "PARAM12"
					_cCont  := "50"
				Case _nx = 13 // valor minimo das parcelas para titulos muito vencidos
					_cParam := "PARAM13"
					_cCont  := "30"
					
			EndCase
			
				Replace ZAO->ZAO_FILIAL With xFilial("ZAO") 
				Replace ZAO->ZAO_PARAM  With _cParam
				Replace ZAO->ZAO_CONT   With _cCont
			ZAO->(MsUnLock())
		EndIF
	Next _nX

EndIf

ZAO->(DbCloseArea())
DbSelectArea("ZAO")
ZAO->(DbSetOrder(1))

If ZAO->(DbSeek(xFilial("ZAO") + "PARAM01   "))
  
	_oCBox1:nAt := Val(ZAO->ZAO_CONT)  
	ZAO->(DbSkip())
	_oCBox2:nAt := Val(ZAO->ZAO_CONT)
	ZAO->(DbSkip())
	_oCBox3:nAt := Val(ZAO->ZAO_CONT)
	ZAO->(DbSkip())
	_cGet1  := ZAO->ZAO_CONT
	ZAO->(DbSkip())
	_cGet2  := ZAO->ZAO_CONT
	ZAO->(DbSkip())
	_cGet3  := ZAO->ZAO_CONT
	ZAO->(DbSkip())
	_nGet4  := Val(ZAO->ZAO_CONT)
	ZAO->(DbSkip())
	_cGet5  := ZAO->ZAO_CONT + Space(1000)
	ZAO->(DbSkip())
	_nGet6  := Val(ZAO->ZAO_CONT)
	ZAO->(DbSkip())
	_dGet7  := STOD(ZAO->ZAO_CONT) // vencimento maximo para titulos muito atrasados 
	ZAO->(DbSkip())
	_nGet8  := val(ZAO->ZAO_CONT) // percentual maximo de desconto para titulos muito atrasados 
	ZAO->(DbSkip())
	_nGet9  := val(ZAO->ZAO_CONT) // percentual minimo para a entrada em titulos muito vencidos
	ZAO->(DbSkip())
	_nGet10 := val(ZAO->ZAO_CONT) // valor minimo das parcelas para titulos muito vencidos
EndIF

ZAO->(DbCloseArea()) 

_oDlg:Refresh()                
	
Return()

