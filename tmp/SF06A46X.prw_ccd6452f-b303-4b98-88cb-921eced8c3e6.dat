Static Function fGravar()

Local _cCont := ""
Local _nX := 1

If MsgYesNo("Confirma a gravação dos parâmetros ?") 

	DbSelectArea("ZAO")
	ZAO->(DbSetOrder(1))
	
	If ZAO->(DbSeek(xFilial("ZAO") + "PARAM01   "))

		While ZAO->(!Eof())
			If RecLock("ZAO",.F.)
				_cCont  := ""
				Do Case
					Case _nX = 1
						_cCont  := AllTrim(Str(_oCBox1:nAt))
					Case _nX = 2
						_cCont  := AllTrim(Str(_oCBox2:nAt))
					Case _nX = 3
						_cCont  := AllTrim(Str(_oCBox3:nAt))
					Case _nX = 4
						_cCont  := _cGet1
					Case _nX = 5
						_cCont  := _cGet2
					Case _nX = 6
						_cCont  := _cGet3
					Case _nX = 7
						_cCont  := AllTrim(Str(_nGet4))
					Case _nX = 8
						_cCont  := _cGet5
					Case _nX = 9
						_cCont  := AllTrim(Str(_nGet6))
					Case _nX = 10
						_cCont  := AllTrim(DTOS(_dGet7))  // vencimento maximo para titulos muito atrasados 
					Case _nX = 11
						_cCont  := AllTrim(cValToChar(_nGet8)) // percentual maximo de desconto para titulos muito atrasados 
					Case _nX = 12
						_cCont  := AllTrim(cValToChar(_nGet9)) // percentual minimo para a entrada em titulos muito vencidos
					Case _nX = 13
						_cCont  := AllTrim(cValToChar(_nGet10)) // valor minimo das parcelas para titulos muito vencidos
				EndCase
				
					Replace ZAO->ZAO_CONT   With _cCont
				ZAO->(MsUnLock())
			EndIF
			ZAO->(DbSkip())
			_nX++
		EndDo
	Else
		Alert("Houve um problema na gravação dos dados!")
	EndIF
Else
	Return(_oDlg)
EndIF

Return()