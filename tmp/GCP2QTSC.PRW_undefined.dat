User Function GCP2QTSC()   
Local np2 := PARAMIXB[9], np3 
Local aRet := {}, aSemUso := {}                      
Local lButSolic  := PARAMIXB[1], nQtde  := PARAMIXB[2], cCodEdt := PARAMIXB[3], ;
      cNumProEdt := PARAMIXB[4], cModalidad := PARAMIXB[5]     
Local cTipoEdt := PARAMIXB[6] , cItemGrid := PARAMIXB[7],  cProdGrid :=   PARAMIXB[8]    


if !(lButSolic .and. __lGCPCNI1)           
 
	np2 := nQtde        
		                 
	if __lGCPCNI1 .and. nQtde > 0      
	                                  
		__aListQtde := U_SICOMA34(10,cCodEdt, cNumProEdt, cModalidad,  cTipoEdt, ;
		                          cItemGrid,   aSemUso ,  __aListQtde,  nQtde, aSemUso ,aSemUso , cProdGrid)     

		np3 := nQtde   //Seta coluna CO2_ORIG
	Else
		return .F.
	EndIf
Else
	return .F.
EndIf	

Aadd(aRet, np2)
Aadd(aRet, np3)

Return (aRet)
