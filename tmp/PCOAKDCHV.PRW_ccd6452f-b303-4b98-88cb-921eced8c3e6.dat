User Function PCOAKDCHV()
Local _cRet := ""
Local _lProcessa := ParamIXB[1] == GetNewPar("SI_XCODPCO","900001")

IF _lProcessa
	_cRet := TMPPCO->AKD_CHAVE
ELSE
	IF ParamIXB[3] // validacao
		_cRet := AKA->AKA_CHAVE
	ELSE // lancamento
		_cRet := ParamIXB[4]
	ENDIF
ENDIF

Return(_cRet)
