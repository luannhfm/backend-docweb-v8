User Function SF0208X(_cChvCN9)
Local _aAreaCN9	:= CN9->(GetArea())
Local _lRet		:= .F.

DBSelectArea("CN9")
DBSetOrder(1)
If CN9->(DBSeek(_cChvCN9))
	_lRet := Iif(AllTrim(CN9->CN9_XREGP) == "1", .T., .F.)
EndIf

RestArea(_aAreaCN9)

Return _lRet

