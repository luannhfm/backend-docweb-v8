::item_contabil_debito_destino  := pItDbDes
::conta_debito_origem           := pCDebOri
::centro_custo_debito_origem    := pCCDbOri
::item_contabil_debito_origem   := pItDbOri
::usuario_aprovacao             := pUsuApr
::valor                         := nValor

Return SELF
