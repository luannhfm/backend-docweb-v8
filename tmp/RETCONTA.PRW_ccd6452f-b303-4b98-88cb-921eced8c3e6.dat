User Function retConta()
Local cRet := ""

If (AllTrim(SEA->EA_MODELO) == "02")
	cRet := "000000000000"
Else
	If (!Empty(SE2->E2_XNUMCON))
		//cRet := PadL(AllTrim(SE2->E2_XNUMCON), 10, "0") + Iif(AllTrim(SEA->EA_MODELO) == "01", AllTrim(SE2->E2_XDVCTA), StrZero(Val(SE2->E2_XDVCTA), 2))
		cRet := PadL(AllTrim(SE2->E2_XNUMCON), 10, "0") + Iif(AllTrim(SEA->EA_MODELO) == "01", Iif(Empty(SE2->E2_XDVCTA), "0 ", AllTrim(SE2->E2_XDVCTA) + Space(2 - Len(AllTrim(SE2->E2_XDVCTA)))), Iif(Empty(SE2->E2_XDVCTA), "00", SE2->E2_XDVCTA))
	Elseif (Empty(SE2->E2_XNUMCON))
		cRet := PadL(AllTrim(SA2->A2_NUMCON), 10, "0") + Iif(AllTrim(SEA->EA_MODELO) == "01", Iif(Empty(SA2->A2_XDVCTA), "0 ", AllTrim(SA2->A2_XDVCTA) + Space(2 - Len(AllTrim(SA2->A2_XDVCTA)))), Iif(Empty(SA2->A2_XDVCTA), "00", SA2->A2_XDVCTA))
	Endif
Endif

Return(cRet)
