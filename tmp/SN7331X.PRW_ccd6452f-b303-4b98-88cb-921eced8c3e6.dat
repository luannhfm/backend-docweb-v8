Static Function MenuDef()

	Local aRotina := {}
	
	ADD OPTION aRotina Title "Pesquisar"  Action "PesqBrw"         OPERATION 1 ACCESS 0
	ADD OPTION aRotina Title "Visualizar" Action "ViewDef.SN7331X" OPERATION 2 ACCESS 0
	ADD OPTION aRotina Title "Incluir"    Action "ViewDef.SN7331X" OPERATION 3 ACCESS 0
	ADD OPTION aRotina Title "Alterar"    Action "ViewDef.SN7331X" OPERATION 4 ACCESS 0
	ADD OPTION aRotina Title "Excluir"    Action "ViewDef.SN7331X" OPERATION 5 ACCESS 0 
 
Return(aRotina)

/*/{Protheus.doc} ModelDef
(long_description)
@author j2a.luizjunior
@since 06/12/2017
@version 1.0
@example
(examples)
@see (links_or_references)
/*/

Static Function ModelDef()
                                                                                               
	Local  oStruZCM := FWFormStruct(1,"ZCM")	
	Local  oModel   := MPFormModel():New("N7331X")
		
	oModel:AddFields("ZCMUNICO", Nil  , oStruZCM)
	oModel:SetDescription("Status Atualizacao Cadastro de Cliente")
	oModel:GetModel("ZCMUNICO"):SetDescription("Status Atualizacao Cadastro de Cliente")      
	oModel:SetPrimaryKey({"ZCM_FILIAL","ZCM_CODATU"})

Return(oModel)

/*/{Protheus.doc} ViewDef
(long_description)
@author j2a.luizjunior
@since 06/12/2017
@version 1.0
@example
(examples)
@see (links_or_references)
/*/

Static Function ViewDef()

	Local oModel   := FWLoadModel("SN7331X")
	Local oStruZCM := FWFormStruct(2,"ZCM")
	Local oView	   := FWFormView():New()
	
	oView:SetModel(oModel)
	oView:AddField("VIEW_ZCM", oStruZCM, "ZCMUNICO")
	oView:CreateHorizontalBox("UM", 100)
	oView:SetOwnerView("VIEW_ZCM" , "UM")

Return(oView)