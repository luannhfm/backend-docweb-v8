MsgRun( "Aguarde, selecionando informações...",, {|| GPE06Qry(.T.)} )

DEFINE MSDIALOG oDlg TITLE "Exclusão de Estrutura de Rateio" FROM aSize[7],00 TO aSize[6],aSize[5] PIXEL
		
	@ 005,005 Say "Mês:"                                                 Of oDlg Pixel
	@ 013,005 ComboBox oMesBase Var cMesBase  Items aMesBase Size 100,36 Of oDlg Pixel

	@ 005,115 Say "Ano:"                                                 Of oDlg Pixel
	@ 013,115 ComboBox oAnoBase Var cAnoBase  Items aAnoBase Size 070,36 Of oDlg Pixel

	@ 013,195 Button "Filtrar"                               Size 040,11 Of oDlg Pixel Action ( GPE06Qry() )

     //------------------ Documentos

	aPosObj[2,1] += 5

	oFolder := TFolder():NEW( aPosObj[2,1],004, { "Centro de Custo / Item Contábil" },, oDlg, 1,,, .t.,, aPosObj[2,4],aPosObj[3,3] )

	@ 005,005 LISTBOX oListItens Fields HEADER;
		  aTitItens[01],;
		  aTitItens[02],;
		  aTitItens[03],;
		  aTitItens[04],;
		  aTitItens[05],;
		  aTitItens[06],;
		  aTitItens[07],;
		  aTitItens[08],;
		  aTitItens[09],;
		  aTitItens[10],;
		  aTitItens[11] SIZE (aPosObj[2,4]-10), (aPosObj[2,3]-aPosObj[2,1]) OF oFolder:aDialogs[1] PIXEL 
			
	oListItens:SetArray( aItens )
	oListItens:bLine   := { || {            aItens[ oListItens:nAT,MN_MES    ]               ,;
									                aItens[ oListItens:nAT,MN_ANO    ]               ,;
									                aItens[ oListItens:nAT,MN_MAT    ]               ,;
									                aItens[ oListItens:nAT,MN_NOME   ]               ,;
									                aItens[ oListItens:nAT,MN_CC     ]               ,;
									                aItens[ oListItens:nAT,MN_DESCCC ]               ,;
									                aItens[ oListItens:nAT,MN_ITEM   ]               ,;
									                aItens[ oListItens:nAT,MN_DESCITE]               ,;
									     Transform( aItens[ oListItens:nAT,MN_HRMES  ], "@E 999"   ) ,;
									     Transform( aItens[ oListItens:nAT,MN_HORAS  ], "@E 999"   ) ,;
									     Transform( aItens[ oListItens:nAT,MN_PERCEN ], "@E 999.99") }}

ACTIVATE MSDIALOG oDlg ON INIT EnchoiceBar( oDlg, {||( Iif( GPE06Val(), oDlg:End(), .T. ) )}, {|| oDlg:End() },, aBotoes )

Return .T.

/*
ÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜ
±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±
±±ÉÍÍÍÍÍÍÍÍÍÍÑÍÍÍÍÍÍÍÍÍÍËÍÍÍÍÍÍÍÑÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍËÍÍÍÍÍÍÑÍÍÍÍÍÍÍÍÍÍÍÍÍÍ»±±
±±ºPrograma  ³ GPE06QRY        ³ Marcelo Coutinho    º Data ³  16/05/14    º±±
±±ÌÍÍÍÍÍÍÍÍÍÍØÍÍÍÍÍÍÍÍÍÍÊÍÍÍÍÍÍÍÏÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÊÍÍÍÍÍÍÏÍÍÍÍÍÍÍÍÍÍÍÍÍÍ¹±±
±±ºDesc.     ³                                                             º±±
±±º          ³                                                             º±±
±±ÌÍÍÍÍÍÍÍÍÍÍØÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍ¹±±
±±ºUso       ³ DTGPE04                                                     º±±
±±ÈÍÍÍÍÍÍÍÍÍÍÏÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍ¼±±
±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±
ßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßß
*/
Static Function GPE06QRY(lInicio)
Local cAliasQry  := GetNextAlias()
Local aAreaAtu   := GetArea()
Local cQuery     := ''
Local nDelItens  := Len( aItens )

Local nSizeCTT   := 4
Local nSizeCTD   := 4
Local nI			:= 0
Default lInicio  := .F.
//--------------------------------------
//
//-----------------------------------------------------------

For nI := 1 To nDelItens

    aDel( aItens, nI )
Next

aSize( aItens, 0 )

//--------------------------------------
//
//-----------------------------------------------------------

cQuery :=        "Select Distinct ZD6.ZD6_MESANO, ZD6.ZD6_MAT, SRA.RA_NOME, ZD6.ZD6_CC, CTT.CTT_DESC01, ZD6.ZD6_ITEM, CTD.CTD_DESC01, ZD6.ZD6_HRMES, ZD6.ZD6_HRTRAB, ZD6.ZD6_PERCEN, ZD6.R_E_C_N_O_ ZD6_RECNO"

cQuery += CRLF + "  From " + RetSqlName("ZD6") + " ZD6 "

cQuery += CRLF + "  Left Join " + RetSqlName("CTT") + " CTT On SubStr( CTT.CTT_FILIAL, 1, " + Str( nSizeCTT, 2 ) + " ) = SubStr( ZD6.ZD6_FILIMP, 1, " + Str( nSizeCTT, 2 ) + ") And CTT.CTT_CUSTO = ZD6.ZD6_CC   And CTT.D_E_L_E_T_ = ' '"
cQuery += CRLF + "  Left Join " + RetSqlName("CTD") + " CTD On SubStr( CTD.CTD_FILIAL, 1, " + Str( nSizeCTD, 2 ) + " ) = SubStr( ZD6.ZD6_FILIMP, 1, " + Str( nSizeCTD, 2 ) + ") And CTD.CTD_ITEM  = ZD6.ZD6_ITEM And CTD.D_E_L_E_T_ = ' '"
cQuery += CRLF + "  Left Join " + RetSqlName("SRA") + " SRA On SRA.RA_MAT     = ZD6.ZD6_MAT  And SRA.D_E_L_E_T_ = ' '"

cQuery += CRLF + " Where ZD6.D_E_L_E_T_ = ' '"
//cQuery += CRLF + "   And ZD6.ZD6_MESANO = '" + cAnoBase + StrZero( Iif( lInicio, Month( dDataBase ), oMesBase:nAt ), 2 ) + "'"
cQuery += CRLF + "   And ZD6.ZD6_MESANO = '" + cPeriodo + "'"

cQuery += CRLF + " Order By ZD6.ZD6_MESANO, ZD6.ZD6_MAT, ZD6.ZD6_CC, ZD6.ZD6_ITEM "

cQuery := ChangeQuery( cQuery )

dbUseArea( .T., "TOPCONN", TCGenQry(,,cQuery), cAliasQry, .T., .T. )     
TCSetField(cAliasQry,"ZD6_HRMES" ,"N",TamSX3("ZD6_HRMES" )[1])
TCSetField(cAliasQry,"ZD6_HRTRAB","N",TamSX3("ZD6_HRTRAB")[1])
TCSetField(cAliasQry,"ZD6_PERCEN","N",TamSX3("ZD6_PERCEN")[1],TamSX3("ZD6_PERCEN")[2])

//--------------------------------------
//
//-----------------------------------------------------------

While (cAliasQry)->(!Eof())

	Aadd( aItens, Array( MN_QTDCOL ) )

	nPosItens := Len( aItens )

	aItens[nPosItens][MN_MES    ] := SubStr( (cAliasQry)->( ZD6_MESANO ), 5, 2 )
	aItens[nPosItens][MN_ANO    ] := SubStr( (cAliasQry)->( ZD6_MESANO ), 1, 4 )
	aItens[nPosItens][MN_MAT    ] :=         (cAliasQry)->( ZD6_MAT    )
	aItens[nPosItens][MN_NOME   ] :=         (cAliasQry)->( RA_NOME    )
	aItens[nPosItens][MN_CC     ] :=         (cAliasQry)->( ZD6_CC     )
	aItens[nPosItens][MN_DESCCC ] :=         (cAliasQry)->( CTT_DESC01 )
	aItens[nPosItens][MN_ITEM   ] :=         (cAliasQry)->( ZD6_ITEM   )
	aItens[nPosItens][MN_DESCITE] :=         (cAliasQry)->( CTD_DESC01 )
	aItens[nPosItens][MN_HRMES  ] :=         (cAliasQry)->( ZD6_HRMES  )
	aItens[nPosItens][MN_HORAS  ] :=         (cAliasQry)->( ZD6_HRTRAB )
	aItens[nPosItens][MN_PERCEN ] :=         (cAliasQry)->( ZD6_PERCEN )
	aItens[nPosItens][MN_RECNO  ] :=         (cAliasQry)->( ZD6_RECNO  )

	(cAliasQry)->(dbSkip())
End

If Len( aItens ) == 0

   Aadd( aItens, Array( MN_QTDCOL ) )

	aItens[Len( aItens )][MN_MES    ] := StrZero( Month( dDataBase ), 2 )
	aItens[Len( aItens )][MN_ANO    ] := StrZero(  YEar( dDataBase ), 4 )
	aItens[Len( aItens )][MN_MAT    ] := ""
	aItens[Len( aItens )][MN_NOME   ] := ""
	aItens[Len( aItens )][MN_CC     ] := ""
	aItens[Len( aItens )][MN_DESCCC ] := ""
	aItens[Len( aItens )][MN_ITEM   ] := ""
	aItens[Len( aItens )][MN_DESCITE] := ""
	aItens[Len( aItens )][MN_HRMES  ] := 0
	aItens[Len( aItens )][MN_HORAS  ] := 0
	aItens[Len( aItens )][MN_PERCEN ] := 0.00
	aItens[Len( aItens )][MN_RECNO  ] := 0
EndIf

(cAliasQry)->(dbCloseArea())

RestArea(aAreaAtu)

Return NIL

/*
ÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜ
±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±
±±ÉÍÍÍÍÍÍÍÍÍÍÑÍÍÍÍÍÍÍÍÍÍËÍÍÍÍÍÍÍÑÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍËÍÍÍÍÍÍÑÍÍÍÍÍÍÍÍÍÍÍÍÍÍ»±±
±±ºPrograma  ³ GPE06Val    ºAutor  ³ Marcelo Coutinho   º Data ³  16/05/14 º±±
