±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±
ßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßß
*/

Static Function GPE06Exc()

Local nX := 0

dbSelectArea( "ZD7" )
dbSetOrder( 2 )

For nX := 1 To Len( aItens )

	ZD6->( dbGoTo( aItens[ nX ][ MN_RECNO ] ) )

	RecLock("ZD6", .F.)		

  	ZD6->( DbDelete() )
	ZD6->( MsUnLock() )

Next

If ZD7->( dbSeek( cAnoBase + StrZero( oMesBase:nAt, 2 ) ) )

	While ZD7->ZD7_MESANO == ( cAnoBase + StrZero( oMesBase:nAt, 2 ) ) .And. ZD7->( !Eof() )

		RecLock("ZD7", .F.)		

  		ZD7->( DbDelete() )
		ZD7->( MsUnLock() )

		ZD7->( dbSkip() )

	End

EndIf

MsgInfo( "Registros excluídos com sucesso!" )

Return .T.

