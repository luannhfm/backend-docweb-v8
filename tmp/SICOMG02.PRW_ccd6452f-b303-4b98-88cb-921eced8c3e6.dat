User Function SICOMG02()

Local aAreaAtu	:= GetArea()
Local aAreaSX5	:= SX5->(GetArea())
Local aAreaZA2	:= ZA2->(GetArea())
Local cCateg	:= ""
Local cRetDesc	:= ""

cCateg := GetAdvFval("ZA4","ZA4_CATEG",xFilial("ZA4")+M->ZA2_CATEG,1,"")

dbSelectArea("SX5")
dbSetOrder(1)
dbSeek(xFilial("SX5")+"X3"+cCateg)
If !Eof()
	cRetDesc := AllTrim(SX5->X5_DESCRI)
EndIf

RestArea(aAreaSX5)
RestArea(aAreaZA2)
RestArea(aAreaAtu)

Return(cRetDesc)