oPrinter:Box(_nPLin	, _nColFour			, _nPLin+_nWidtLi	, _nColFnl)
oPrinter:SayAlign(_nPLin+2, nMargDir+485, "Assinatura "			, _oFntC8, 160, 200, , 0)

Return (Nil)

/*/{Protheus.doc} CabItem
	Função para impressão do cabeçalho dos itens

@author Franklin de Brito de Oliveira
@since 26/06/2015
@version 1.0
@param oPrinter, objeto, Objeto de impressão

@return Nil

/*/
//-------------------------------------------------------------------------
/*
Modificações:
	14/10/2015 - Franklin B Oliveira:
	->Adicionado tratamento para impressão da Unidade Destino, quando o programa
	de origem for transferência multipla.

*/
//--------------------------------------------------------------------------
Static Function CabItem(oPrinter)
//Tabela
Local _nColTwo	:= 50
Local _nColThree:= 130
Local _nColFour	:= 210
Local _nColFive	:= 400
Local _nColSix	:= 500
Local _nColFnl	:= 580
Local _nWidtLi	:= 10
Local _nPLin	:= 220
//Entidade...
Local _nLinC1	:= 75
Local _nCl1C1	:= 200
Local _nCl2C1	:= 395
Local _nCl3C1	:= 580
Local _nHeigC1	:= 30
//Unid Dest...
Local _nLinC2	:= 110
Local _nCl1C2	:= 300
Local _nCl2C2	:= 580
//Tit Tabela
Local _nLi0C3	:= 145
Local _nLinC3	:= 160
Local _nLi2C3	:= 185
Local _nCl1C3	:= 205
Local _nCl2C3	:= 395
Local _nHeigC3	:= 20
//Variaveis Informações
Local _cMotBx	:= "CESSÃO TEMPORÁRIA"
Local _cInfo	:=	IIf( _cTpCab $ 'E',"Os bens patrimoniais abaixo relacionados ficam sob inteira e exclusiva responsabilidade "+;
					"do destinatário durante o período que estiver sob guarda.", "Os bens abaixo relacionados estão sendo devolvidos.")
Local _cFil := (_cAlias)->ZEC_MSFIL

// Entidade / Unidade Cedente / Setor Cedente
oPrinter:Box(li+_nLinC1	, nMargDir	, li+_nLinC1+_nHeigC1	, _nCl1C1)
oPrinter:SayAlign(li+80 , nMargDir+60	, "Entidade"		, _oFntC8, 160, 200, , 0)
oPrinter:SayAlign(li+88 , nMargDir+60	, _cFil				, _oFntC8, 160, 200, , 0)

oPrinter:Box(li+_nLinC1	, _nCl1C1	, li+_nLinC1+_nHeigC1	, _nCl2C1)
oPrinter:SayAlign(li+80 , _nCl1C1+50	, "Unidade Cedente"				, _oFntC8, 160, 200, , 0)
oPrinter:SayAlign(li+88 , _nCl1C1+50	,  RTrim(Posicione("SM0",1, cEmpAnt + _cFil,"M0_FILIAL"))				, _oFntC8, 160, 200, , 0)

oPrinter:Box(li+_nLinC1	, _nCl2C1	, li+_nLinC1+_nHeigC1	, _nCl3C1)
oPrinter:SayAlign(li+80 , _nCl2C1	, "Setor Cedente"		, _oFntC8, 180, 200, 2, 2)
oPrinter:SayAlign(li+88, _nCl2C1	, SubStr(Posicione("SNL", 1, xFilial("SNL") + (_cAlias)->ZEA_LOCAL, "NL_DESCRIC"), 1, 40), _oFntC8, 180, 200, 2, 2)

//Wjr Controle Cabeçalhos
//Se Empregado
If _cTpEmp == 'EM'
	oPrinter:Box(li+_nLinC2	, nMargDir	, li+_nLinC2+_nHeigC1	, _nCl1C2)
	oPrinter:SayAlign(li+115, nMargDir	, "Nome Completo"		, _oFntC8, 260, 200, 2, 2)
	oPrinter:SayAlign(li+123, nMargDir	, (_cAlias)->NOMEENT	, _oFntC8, 260, 200, 2, 2)
	
	oPrinter:Box(li+_nLinC2	, _nCl1C2	, li+_nLinC2+_nHeigC1	, _nCl2C2)
	oPrinter:SayAlign(li+115, _nCl1C2	, "CPF"					, _oFntC8, 260, 200, 2, 2)
	oPrinter:SayAlign(li+123, _nCl1C2	, Transform(AllTrim((_cAlias)->CGCENT), Iif(Len(AllTrim((_cAlias)->CGCENT))==11,;
												"@R 999.999.999-99", "@R 99.999.999/9999-99"))	, _oFntC8, 260, 200, 2, 2)

//Se Pessoa Publica
ElseIf _cTpEmp == 'PU'
	oPrinter:Box(li+_nLinC2	, nMargDir		, li+_nLinC2+_nHeigC1	, _nCl1C2-130)
	oPrinter:SayAlign(li+115, nMargDir+5	, "Razão Social"	, _oFntC8, 260, 20, 0, 0)
	oPrinter:SayAlign(li+123, nMargDir+5	, (_cAlias)->NOMEENT , _oFntC8, 260, 20, 0, 0)
	
	oPrinter:Box(li+_nLinC2	, _nCl1C2-130	, li+_nLinC2+_nHeigC1	, _nCl1C2)
	oPrinter:SayAlign(li+115, nMargDir+160	, "CNPJ"	, _oFntC8, 260, 20, 0, 0)
	oPrinter:SayAlign(li+123, nMargDir+160	, Transform(AllTrim((_cAlias)->CGCENT), Iif(Len(AllTrim((_cAlias)->CGCENT))==11,;
												"@R 999.999.999-99", "@R 99.999.999/9999-99")), _oFntC8, 260, 20, 0, 0)
	
	oPrinter:Box(li+_nLinC2	, _nCl1C2		, li+_nLinC2+_nHeigC1	, _nCl1C2+150)
	oPrinter:SayAlign(li+115, _nCl1C2+5		, "Responsavel"			, _oFntC8, 260, 20, 0, 0)
	oPrinter:SayAlign(li+123, _nCl1C2+5		, (_cAlias)->ZEC_TRCIRO , _oFntC8, 260, 200, 2, 0)
	
	oPrinter:Box(li+_nLinC2	, _nCl1C2+150	, li+_nLinC2+_nHeigC1	, _nCl1C2+280)
	oPrinter:SayAlign(li+115, _nCl1C2+155	, "Numero de Contrato"	, _oFntC8, 260, 20, 0, 0)
