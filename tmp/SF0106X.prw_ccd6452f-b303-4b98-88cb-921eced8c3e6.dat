Static Function GerQuery()
Local _cQuery	:= ""
Local _nx		:= 0
Local _nCnt		:= .F.

_cQuery := " Select ZEC.ZEC_MSFIL, " + CRLF
_cQuery += "	ZEC.ZEC_FILDES, " 	+ CRLF
_cQuery += "	ZEC.ZEC_CODIGO, " 	+ CRLF
_cQuery += "	ZEC_LOCDES, " + CRLF
_cQuery += "	ZEC_CODRES, " + CRLF
_cQuery += "	ZEC_NROCTO, " + CRLF
_cQuery += "	ZEC_TRCIRO, " + CRLF
_cQuery += "	ZED.ZED_CODBEM, " + CRLF
_cQuery += "	ZEA.ZEA_DESC, " + CRLF
_cQuery += "	ZEA.ZEA_DTAQUI, " + CRLF
_cQuery += "	ZEA.ZEA_PRCAQS, " + CRLF
_cQuery += "	ZEA.ZEA_LOCAL, " + CRLF
If cTpEnt == 'C'
	_cQuery += "	SA1.A1_NOME NOMEENT, " + CRLF 
	_cQuery += "	SA1.A1_CGC CGCENT" + CRLF
Else
	_cQuery += "	SA2.A2_NOME NOMEENT, " + CRLF 
	_cQuery += "	SA2.A2_CGC CGCENT" + CRLF
EndIf
_cQuery += " FROM " + RetSQLName("ZEC") + " ZEC " + CRLF
_cQuery += "INNER JOIN " + RetSQLName("ZED") + " ZED ON ZED.D_E_L_E_T_ = ' ' AND ZED.ZED_FILIAL = ZEC.ZEC_FILIAL AND ZED.ZED_CODIGO = ZEC.ZEC_CODIGO " + CRLF
_cQuery += "INNER JOIN " + RetSQLName("ZEA") + " ZEA ON ZEA.D_E_L_E_T_ = ' ' AND ZEA.ZEA_FILIAL = '" + xFilial("ZEA") + "' AND ZEA.ZEA_CODIGO = ZED.ZED_CODBEM " + CRLF
If cTpEnt == 'C'
	_cQuery += "LEFT JOIN  " + RetSQLName("SA1") + " SA1 ON SA1.D_E_L_E_T_ = ' ' AND SA1.A1_FILIAL = '" + xFilial("SA1") + "' AND SA1.A1_COD = ZEC.ZEC_CODCLI AND SA1.A1_LOJA = ZEC.ZEC_LOJA " + CRLF
Else
	_cQuery += "LEFT JOIN  " + RetSQLName("SA2") + " SA2 ON SA2.D_E_L_E_T_ = ' ' AND SA2.A2_FILIAL = '" + xFilial("SA2") + "' AND SA2.A2_COD = ZEC.ZEC_CODCLI AND SA2.A2_LOJA = ZEC.ZEC_LOJA " + CRLF
EndIf
_cQuery += " WHERE " + CRLF
_cQuery += " ( " + CRLF
For _nx := 1 To Len(_aMovSel)
	_cQuery += Iif( !_nCnt, " ( ", " Or ( ")
	_cQuery += " 	ZEC.ZEC_MSFIL	= '"+ _aMovSel[_nx, 1]+"' AND " + CRLF
	_cQuery += " 	ZEC.ZEC_CODIGO	= '"+ _aMovSel[_nx, 2]+"' AND " + CRLF
	_cQuery += " 	ZED.ZED_ITEM	= '"+ _aMovSel[_nx, 3]+"' AND " + CRLF
	_cQuery += " 	ZED.ZED_CODBEM	= '"+ _aMovSel[_nx, 4]+"' " + CRLF
	_cQuery += " ) " + CRLF
	_nCnt := .T.
Next
_cQuery += " ) AND " + CRLF
_cQuery += " 	ZEC.D_E_L_E_T_ = ' ' " + CRLF
_cQuery += " Order By	1,2,3 "

MemoWrite("C:\temp\"+FunName()+"_"+ProcName()+".TXT",_cQuery)

DBUseArea(.T., "TOPCONN", TcGenQry(,,ChangeQuery(_cQuery)),_cAlias, .F.,.F.)

Return (_cAlias)