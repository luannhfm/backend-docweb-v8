#include 'protheus.ch'

/*/{Protheus.doc} SF73A01F
Função para retornar o código IBGE do estado
@author Franklin Oliveira
@since 22/01/2020
@param pSiglaEst, caracter, Sigla do estado
@return cCodIBGE, caracter, Código IBGE do estado

@type User function
/*/
User Function SF73A01F(pSiglaEst)
Local cCodIBGE	:= ""

Default	pSiglaEst := ""
	
	pSiglaEst := Upper(pSiglaEst)
	
	Do Case
		Case pSiglaEst == "AC"
			cCodIBGE := "12"
		Case pSiglaEst == "AL"
			cCodIBGE := "27"
		Case pSiglaEst == "AP"
			cCodIBGE := "16"
		Case pSiglaEst == "AM"
			cCodIBGE := "13"
		Case pSiglaEst == "BA"
			cCodIBGE := "29"
		Case pSiglaEst == "CE"
			cCodIBGE := "23"
		Case pSiglaEst == "DF"
			cCodIBGE := "53"
		Case pSiglaEst == "ES"
			cCodIBGE := "32"
		Case pSiglaEst == "GO"
			cCodIBGE := "52"
		Case pSiglaEst == "MA"
			cCodIBGE := "21"
		Case pSiglaEst == "MT"
			cCodIBGE := "51"
		Case pSiglaEst == "MS"
			cCodIBGE := "50"
		Case pSiglaEst == "MG"
			cCodIBGE := "31"
		Case pSiglaEst == "PA"
			cCodIBGE := "15"
		Case pSiglaEst == "PB"
			cCodIBGE := "25"
		Case pSiglaEst == "PR"
			cCodIBGE := "41"
		Case pSiglaEst == "PE"
			cCodIBGE := "26"
		Case pSiglaEst == "PI"
			cCodIBGE := "22"
		Case pSiglaEst == "RN"
			cCodIBGE := "24"
		Case pSiglaEst == "RS"
			cCodIBGE := "43"
		Case pSiglaEst == "RJ"
			cCodIBGE := "33"
		Case pSiglaEst == "RO"
			cCodIBGE := "11"
		Case pSiglaEst == "RR"
			cCodIBGE := "14"
		Case pSiglaEst == "SC"
			cCodIBGE := "42"
		Case pSiglaEst == "SP"
			cCodIBGE := "35"
		Case pSiglaEst == "SE"
			cCodIBGE := "28"
		Case pSiglaEst == "TO"
			cCodIBGE := "17"
	EndCase
	
Return cCodIBGE