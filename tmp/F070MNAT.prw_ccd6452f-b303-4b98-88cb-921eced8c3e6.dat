User Function F070MNAT()
	Local aArea := {GetArea(), SE1->(GetArea())}
	Local lRet := .F.

	If !IsBlind()

		If (SE1->(E1_MULTNAT) == "1")
			lRet := .T.
		Endif

	Endif

	aEval(aArea, {|x| RestArea(x)})

Return(lRet)
