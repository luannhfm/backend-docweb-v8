WSRESTFUL contacontabil DESCRIPTION 'endpoint Contas Contabeis - VENUS ' FORMAT "application/json,text/html"
    WSDATA Page              AS INTEGER OPTIONAL
    WSDATA aQueryString      AS ARRAY    OPTIONAL
    
 	WSMETHOD GET contacontabil;
	    DESCRIPTION "Retorna uma lista de Contas Contabeis";
	    WSSYNTAX "/api/contacontabil" ;
        PATH "/api/contacontabil" ;
	    PRODUCES APPLICATION_JSON
 	
END WSRESTFUL

//-------------------------------------------------------------------
/*/{Protheus.doc} GET LancContabil
MÃ©todo GET contacontabil
