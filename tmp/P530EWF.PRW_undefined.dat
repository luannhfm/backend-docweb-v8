User Function P530EWF()
Local _aEmail := ParamIXB[2]

For i := 1 to Len(_aEmail)
	_cBody := "Prezado "+ALI->ALI_NOME+Chr(13)+Chr(10)+Chr(13)+Chr(10)
	_cBody += "Foi gerada a contingência Nr. "+ALI->ALI_CDCNTG+" para sua aprovação."+Chr(13)+Chr(10)+Chr(13)+Chr(10)
	_cBody += "Data da Solicitação : "+Dtoc(ALI->ALI_DTSOLI)+"  -  Hora :"+ALI->ALI_HORA+Chr(13)+Chr(10)+Chr(13)+Chr(10)
	_cBody += "Solicitante : "+ALI->ALI_NOMSOL
	ACSendMail( ,,,,_aEmail[i],"Aviso de Contingencia",_cBody)
Next

Return()
